{
  "name": "neos-code-b",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "npx babel src --extensions '.ts,.tsx,.json' -d dist --watch",
    "setup": "node ./src/lib/builder/componentSetup.cjs && prettier --write ./src/lib/core/components",
    "buildTs": "npx tsc",
    "babel": "npx babel build -d dist",
    "build": "npm run buildTs && npm run babel && node --experimental-json-modules ./dist/main.js",
    "start": "npx babel src/ src/index.tsx src/main.tsx --out-dir dist --extensions '.ts,.tsx,.json' && node --experimental-json-modules ./dist/main.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rehype-parse": "^8.0.3",
    "rehype-stringify": "^9.0.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.1",
    "uuid": "^8.3.2"
  }
